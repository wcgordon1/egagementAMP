---
import BaseLayout from "../layouts/BaseLayout.astro";
import Hero from "../components/landing/Hero.astro";
import FeatureOne from "../components/landing/FeatureOne.astro";
import FeatureTwo from "../components/landing/FeatureTwo.astro";
import FeatureThree from "../components/landing/FeatureThree.astro";
import Cta from "../components/landing/Cta.astro";
import FeatureFour from "../components/landing/FeatureFour.astro";
import Testimonial from "../components/landing/Testimonial.astro";
import Pricing from "../components/landing/Pricing.astro";
import Faq from "../components/landing/Faq.astro";
import Footer from "../components/global/Footer.astro";
import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="Flabbergasted || home"
  description="Your site description goes here"
  canonical="https://www.supremeuikits.com"
  openGraph={{
    url: "https://www.supremeuikits.com",
    title: "Open Graph Title",
    description: "Open Graph Description",
    images: [
      {
        url: "https://www.supremeuikits.com/opengraph/social-og.jpg",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://www.supremeuikits.com/opengraph/twitter-og.jpg",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://www.supremeuikits.com/opengraph/social-og.jpg" },
      { url: "https://www.supremeuikits.com/opengraph/twitter-og.jpg" },
    ],
    site_name: "YourSitesName",
  }}
  twitter={{
    handle: "@TwitterHandle",
    site: "@site",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
  <Hero />
  <FeatureOne />
  <FeatureTwo />
  <FeatureThree />
  <Cta />
  <FeatureFour />
  <Testimonial />
  <Pricing />
  <Faq />
  <Footer />
</BaseLayout>
